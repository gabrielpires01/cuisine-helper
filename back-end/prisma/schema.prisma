generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id        Int        @id @default(autoincrement())
  name String
  email     String     @unique
  password  String
  createdAt DateTime   @default(now())
  sessions  sessions[]
}

model sessions {
  id        Int      @id @default(autoincrement())
  token     String   @unique
  userId    Int?
  createdAt DateTime @default(now())
  users     users?   @relation(fields: [userId], references: [id], onDelete: NoAction, onUpdate: NoAction)
}